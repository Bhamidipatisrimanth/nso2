Host bastionNSO
  User ubuntu
  HostName 91.106.193.76
  IdentityFile ~/.ssh/id_rsa
  UserKnownHostsFile=~/dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  ForwardAgent yes

